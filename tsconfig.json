{
  "compilerOptions": {
    "outDir": "./.tsbuild/",
    "module": "commonjs",
    "target": "es2018",
    "types": [
        "node",
        "webdriverio/sync",
        "@wdio/mocha-framework",
        "expect-webdriverio"
    ],
    "moduleResolution": "node",
    "checkJs": true,
  },
  "include": ["**/*.ts", "**/*.json", "wdio.conf.js"],
  "exclude": ["node_modules"]
}
